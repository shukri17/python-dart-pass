
abstract class Animal {
  printName();
  printSound();
}

class Dog extends Animal {
  @override
  printName() {
    print("name :Dog ");
  }

  @override
  printSound() {
    print("Dog Sound Braking");
  }
}

class Cat extends Animal {
  @override
  printName() {
    print("Name :Cat");
  }

  @override
  printSound() {
    print("Cat Sound meaw");
  }
}

class Cow extends Animal {
  @override
  printName() {
    print("Name : Cow ");
  }

  @override
  printSound() {
    print("Cow sound is  moo");
  }
}

main() {

  var dog = Dog();
  dog.printName();
  dog.printSound();
  var c1 = Cat();
  c1.printName();
  c1.printSound();
  var c2= Cow();
  c2.printName();
  c2.printSound();
}
